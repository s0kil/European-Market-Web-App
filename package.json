{
  "private": true,
  "name": "european-market-web-app",
  "description": "European Market Web App Built With Sapper + Svelte",
  "version": "0.0.1",
  "scripts": {
    "dev": "PORT=8000 sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "prod": "NODE_ENV=production npm run export && npm run subFont",
    "subFont": "subfont __sapper__/export --in-place --recursive --inline-css",
    "serveExport": "python3 -m http.server --directory __sapper__/export/ 8000"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "polka": "next",
    "sirv": "next"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/runtime": "^7.9.6",
    "@fabiohvp/svelte-table": "^1.6.7",
    "@reshadow/svelte": "^0.0.1-alpha.76",
    "@rollup/plugin-babel": "^5.0.2",
    "@rollup/plugin-commonjs": "12.0.0",
    "@rollup/plugin-image": "^2.0.5",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@s0kil/tiny-map": "0.1.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "flag-icon-css": "^3.4.6",
    "loadjs": "^4.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "modern-normalize": "^0.6.0",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^1.1.0",
    "rollup": "^2.10.9",
    "rollup-plugin-svelte": "^5.2.2",
    "rollup-plugin-terser": "^6.0.1",
    "sapper": "^0.27.13",
    "sirv-cli": "^0.4.6",
    "sitemap-generator": "^8.4.2",
    "subfont": "^5.0.0",
    "supports-webp": "^2.0.1",
    "svelte": "^3.22.3",
    "svelte-loader": "^2.13.6",
    "webpack": "^4.43.0"
  },
  "subsets": {
    "netlify": {
      "include": [
        "netlify-cli"
      ]
    }
  }
}
