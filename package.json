{
  "private": true,
  "name": "european-market-web-app",
  "description": "European Market Web App Built With Sapper + Svelte",
  "version": "0.0.1",
  "scripts": {
    "dev": "PORT=8000 sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "prod": "NODE_ENV=production npm run export && npm run subFont",
    "subFont": "subfont __sapper__/export --in-place --recursive --inline-css",
    "serveExport": "sirv --port 8000 __sapper__/export"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "polka": "next",
    "sirv": "next"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/runtime": "^7.8.7",
    "@reshadow/svelte": "^0.0.1-alpha.74",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-image": "^2.0.4",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.1",
    "@s0kil/tiny-map": "0.1.1",
    "ackee-tracker": "^3.2.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "flag-icon-css": "^3.4.6",
    "instant.page": "^3.0.0",
    "loadjs": "^4.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "modern-normalize": "^0.6.0",
    "prettier": "^1.19.1",
    "prettier-plugin-svelte": "^0.7.0",
    "rollup": "^2.1.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-svelte": "^5.1.1",
    "rollup-plugin-terser": "^5.3.0",
    "sapper": "^0.27.11",
    "sirv-cli": "^0.4.5",
    "sitemap-generator": "^8.4.2",
    "subfont": "^4.2.0",
    "supports-webp": "^2.0.1",
    "svelte": "^3.20.1",
    "svelte-loader": "^2.13.6",
    "webpack": "^4.42.0"
  },
  "subsets": {
    "netlify": {
      "include": [
        "netlify-cli"
      ]
    }
  }
}
