{
  "private": true,
  "name": "european-market-web-app",
  "description": "European Market Web App Built With Sapper + Svelte",
  "version": "0.0.1",
  "scripts": {
    "dev": "PORT=8000 sapper dev",
    "build": "NODE_ENV=production sapper build",
    "export": "NODE_ENV=production sapper export",
    "prod": "npm run export && npm run subFont",
    "checkTypes": "cd src; npx svelte-type-checker",
    "subFont": "subfont __sapper__/export --in-place --recursive --inline-css",
    "optimizeImages": "imager -i static/images/**/*.jpg -o static/images/min/",
    "serveExport": "simple-http-server -i -t=8 --nosort --nocache __sapper__/export"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "polka": "next",
    "sirv": "next"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@reshadow/svelte": "^0.0.1-alpha.72",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-image": "^2.0.2",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.0",
    "@rollup/plugin-replace": "^2.3.1",
    "@s0kil/tiny-map": "0.1.1",
    "ackee-tracker": "^3.2.2",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "flag-icon-css": "^3.4.5",
    "instant.page": "^3.0.0",
    "loadjs": "^4.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "modern-normalize": "^0.6.0",
    "prettier": "^1.19.1",
    "prettier-plugin-svelte": "^0.7.0",
    "rollup": "^1.31.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-svelte": "^5.1.1",
    "rollup-plugin-terser": "^5.2.0",
    "sapper": "^0.27.9",
    "sitemap-generator": "^8.4.2",
    "subfont": "^4.1.2",
    "svelte": "^3.18.1",
    "svelte-loader": "^2.13.6",
    "svelte-type-checker": "^0.1.4",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5"
  },
  "subsets": {
    "netlify": {
      "include": [
        "netlify-cli"
      ]
    }
  }
}
